# API Keys for different providers
SILICONFLOW_API_KEY=
BAILIAN_API_KEY=
QWEN_API_KEY=
BYTEDANCE_API_KEY=

# API Service Configuration
API_PROVIDER=siliconflow  # 可选值: siliconflow, bailian

API_BASE_URL=https://api.siliconflow.cn/v1
API_CHAT_PATH=/chat/completions  # SiliconFlow的路径
  # 当 API_PROVIDER=siliconflow 时使用
BAILIAN_API_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1  # 当 API_PROVIDER=bailian 时使用
BAILIAN_API_CHAT_PATH=/chat/completions  # 百炼的路径

# 在现有配置后添加
QWEN_API_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
QWEN_API_CHAT_PATH=/chat/completions

# 在现有配置后添加 Bytedance 的配置
BYTEDANCE_API_BASE_URL=https://ark.cn-beijing.volces.com/api/v3  #  请替换为 Bytedance API 的基础 URL
BYTEDANCE_API_CHAT_PATH=/chat/completions  #  请替换为 Bytedance API 的 chat completions 路径，如果不同的话
